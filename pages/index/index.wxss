page,.box {
  margin: 0;
  padding: 0;
  width: 100%;
  height: 100%;
}
.header {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 80px;
}
.header .user {
  position: absolute;
  width: 30px;
  height: 30px;
  left: 20px;
}
.main {
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
}
.item {
  display: flex;
  flex-direction: row;
  background-color: aliceblue;
  width: 80%;
  height: 100px;
  border-radius: 15px;
  margin: 10px;
  align-items: center;
}
.item:nth-child(1) {
  margin: 0px 0px 10px 0px;
}
.item_image {
  margin: 0px 20px;
}
.item_image cover-image {
  width: 75px;
  height: 75px;
}
.item_subtitle {
  margin-top: 12px;
  font-size: 10px;
  color: rgb(70, 70, 70);
}
